{
  "name": "@caviar/plugin-apollo-env",
  "version": "3.0.2",
  "description": "Caviar plugin to apply configurations from Ctrip's apollo config service to process.env",
  "main": "src/index.js",
  "scripts": {
    "test": "nyc ava --timeout=10s",
    "test-no-report": "NODE_DEBUG=@caviar/plugin-apollo-env nyc ava --timeout=10s --verbose",
    "lint": "eslint .",
    "posttest": "nyc report --reporter=text-lcov > coverage.lcov && codecov"
  },
  "files": [
    "src/"
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/kaelzhang/@caviar/plugin-apollo-env.git"
  },
  "keywords": [
    "@caviar/plugin-apollo-env",
    "Caviar-plugin",
    "apollo"
  ],
  "engines": {
    "node": ">= 8.6.0"
  },
  "ava": {
    "babel": false
  },
  "author": "kaelzhang",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kaelzhang/@caviar/plugin-apollo-env/issues"
  },
  "devDependencies": {
    "ava": "^1.4.1",
    "codecov": "^3.3.0",
    "nyc": "^14.0.0",
    "eslint-config-ostai": "^3.0.0",
    "eslint-plugin-import": "^2.17.2",
    "eslint": "^5.16.0"
  },
  "dependencies": {
    "ctrip-apollo": "^4.1.1",
    "err-object": "^4.3.1",
    "has-own-prop": "^1.0.1",
    "mix2": "^1.0.3"
  }
}
